{
  "manifest_version": 3,
  "name": "Google Logo Click Extension",
  "version": "1.0",
  "description": "Detects clicks on the Google logo and changes the background color.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  }
}
